#include <stdio.h>
#include<stdlib.h>

int main()
{
  int num, i, *ptr, sum=0;

  printf("Enter the number of elements: ");
  scanf("%d", &num);
  ptr= (int*) malloc (num*sizeof(int));
  printf("Enter %d elements :\n",num);
  for(i=0;i<num;i++)
  {
     scanf("%d",ptr+i);
     sum += *(ptr+i);
  }
  printf("Sum= %d",sum);
  return 0;
}
